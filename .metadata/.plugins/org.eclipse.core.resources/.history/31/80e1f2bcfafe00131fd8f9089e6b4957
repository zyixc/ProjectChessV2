<script>  

var moves = getUrlVars()["moves"];
moves = moves.replace(/%20/g," ");

moves = moves.substring(0, moves.lastIndexOf(" ");

function getUrlVars() {
    var vars = {};
    var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
        vars[key] = value;
    });
    return vars;
}

new PgnViewer(  
  { boardName: "demo",  
  //  pgnFile: '/kasparov.pgn',  
	pgnString: moves,
	pieceSet: 'merida',   
    pieceSize: 46  
  }  
);  
</script>  


<body>

<div id="demo-container"></div>  
<div id="demo-moves"></div>  


</body>

